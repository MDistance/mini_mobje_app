<view class="container">
  <view class="header">
    <text class="header-title">浦东机场T2店{{}}</text>
  </view>
   <view class="main">
       <view class="content">
        <view class="main-header"><text>网点显示</text></view>
        <view class="main-con">
          <scroll-view class="scrollItem" scroll-x="true">
              <view class="wrap">
                <block wx:for="{{list}}" wx:key="index">
                    <view class="wrapList" bindtap="jump">
                        <image src="{{item.image}}"></image>
                      </view>
                  </block> 
              </view>
          </scroll-view>
          <view class="address">
            <view class="openHours addressTo">
              <text class="addressDesc">上海市浦东新区苏杭路110号{{}}</text>
              <text class="iconfont icon-zb" style="color:blue" bindtap="toNavgiat"></text>
            </view>
            <view class="openHours">
              <text class="openHours-tit">营业时间 : </text>
              <text class="openHours-time">9:00-16:00</text>
            </view>
            <view class="phoneNum">
              <text class="openHours-tit">网点联系人 : </text>
              <text class="openHours-time">张三</text>
            </view>
            <view class="phoneNum">
              <text class="openHours-tit">联系人电话 : </text>
              <text class="openHours-time">18812345678</text>
            </view>
          </view>
        </view>

       </view>
   </view>
  <view class="footer">
    <text class="footerText" bindtap="addInvoiceTitle">选此网点</text>
  </view>
</view>