.container{
  width: 100%;
  height: 100%;
  overflow: hidden;
  overflow-y: scroll;
}
.header{
  background-color: #fff;
  padding: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-bottom: 20rpx;
}
.header-item{
  flex-direction: column;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.content{
  width: 100%;
  height: 80%;
  padding: 30rpx;
  background-color: #fff;
}
.cont_count{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}