<view class="container">
  <view class="header">
    <view class="header-title">
    </view>
  </view>
   <view class="main"  wx:if="{{invoiceNum.length>0}}">
     <view class="main-con" wx:for="{{invoiceNum}}" wx:key="key">
          <van-cell-group>
            <van-field
              value="{{ username }}"
              readonly
              label="订单号"
              placeholder="12434242424354654"

            />
            <van-field
              value="6677"
              label="发票金额"
              readonly
            />
            <van-field
              value="{{ password }}"
              type="password"
              label="开票时间"
              readonly
              placeholder="2020-11-04 8:00"
            />
          
            <van-field
              value="{{ username }}"
              readonly
              label="手机号"
            />
            <van-field
              value="{{ password }}"
              readonly
              label="电子邮箱"
            />
            <van-field
              value="{{ username }}"
              readonly
              label="企业名称"
            />
            <van-field
              value="{{ password }}"
              type="password"
              label="开户行"
              readonly
            />
            <van-field
              value="{{ password }}"
              type="password"
              label="地址"
              readonly
            />
            <van-field
              value="{{ username }}"
              clearable
              label="税号"
              readonly
            />
          </van-cell-group>
     </view>
    
   </view>
   <view wx:else class="main-title">
        暂无发票历史
    </view>
</view>