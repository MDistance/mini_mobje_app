<!--pages/networkNum/networkNum.wxml-->
<view class="container">
  <view class="header" style=" position: fixed;top:0">
    <!-- <form action=""> -->
      <view class="weui-cell__bd">
        <text class="iconfont icon-suosou"></text>
        <input class="weui-input"  auto-focus type="text" bindinput="bindKeyInput" placeholder="请输入网点名称" />
      </view>
      <view class="sug_info"> 
        <text>{{sugData}}</text> 
      </view>
      <!-- <text class="map">地图</text> -->
    <!-- </form> -->
  </view>
  <view class="content">
    <view class="map_container"> 
      <map class="map" id="map" longitude="{{longitude}}" latitude="{{latitude}}" scale="14" show-location="true" markers="{{markers}}" bindmarkertap="makertap"></map> 
    </view> 
    <view class="place_info"> 
      <text>{{placeData.title}}</text> 
      <text>{{placeData.address}}</text> 
      <text>{{placeData.telephone}}</text> 
    </view> 
  </view>
    
</view>
