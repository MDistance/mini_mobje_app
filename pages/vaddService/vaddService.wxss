/* pages/vaddService/vaddService.wxss */

.container{
  width: 100%;
  height: 100%;
  background-color: paleturquoise;
}
.header{
  z-index: 999;
  overflow: hidden;
  position: fixed;
  top: 0rpx;
  background-color: white;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 200rpx;
  border-bottom: 1px solid seashell;
}
.main{
  border: 1px solid seashell;
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  padding: 30rpx;
  background-color: #fff;
  margin-bottom: 30rpx;
}

.contentHeader{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: 1rpx seashell solid;
}
.van-cell-group ,.van-cell{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.read{
  padding: 30rpx;
  display: flex;
  justify-content: start;
}
.van-radio-groupRead{
  display: inline-block;
}
.ruleText{
  color: orange;
}
.footer{
  background-color: #fff;
}
/* 提交订单按钮样式 */

.subBtn{
  z-index: 999;
  border-top: whitesmoke 1rpx solid;
  background-color: white;
  width: 100%;
  height: 80rpx;
  position: fixed;
  bottom: 0;
  padding: 20rpx 0;
  display: flex;
  align-items: center;
  justify-content: center;
  
}
.subBtnText{
  border-radius: 20rpx;
  width: 90%;
  height: 70rpx;
  line-height: 70rpx;
  background-color: orange;
  color: white;
  text-align: center;
}